{
  "name": "kuromoji",
  "description": "JavaScript implementation of Japanese morphological analyzer",
  "version": "0.1.2",
  "author": "Takuya Asano <takuya.a@gmail.com>",
  "browser": {
    "./src/loader/NodeDictionaryLoader.js": "./src/loader/BrowserDictionaryLoader.js"
  },
  "bugs": {
    "url": "https://github.com/takuyaa/kuromoji.js/issues"
  },
  "dependencies": {
    "async": "^3.2.5",
    "doublearray": "0.0.2",
    "zlibjs": "^0.3.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.1.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.10",
    "coveralls-next": "^4.2.0",
    "del": "^7.1.0",
    "eslint": "^8.53.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-connect": "^5.7.0",
    "gulp-conventional-changelog": "^4.0.0",
    "gulp-gh-pages-will": "^0.5.5",
    "gulp-git": "^2.10.1",
    "gulp-gzip": "^1.4.2",
    "gulp-mocha": "^10.0.0",
    "jsdoc": "^4.0.2",
    "mecab-ipadic-seed": "0.0.2",
    "merge-stream": "^2.0.0",
    "minimist": "^1.2.8",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "run-sequence": "^2.2.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "resolutions": {
    "lodash": "4.17.21"
  },
  "homepage": "https://github.com/takuyaa/kuromoji.js",
  "keywords": [
    "japanese",
    "morphological analyzer",
    "nlp",
    "pos",
    "pos tagger",
    "tokenizer"
  ],
  "license": "Apache-2.0",
  "main": "./src/kuromoji.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/takuyaa/kuromoji.js.git"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "gulp build_task",
    "build-demo": "gulp copy_demo_task && npm install --prefix publish/demo",
    "build-dict": "gulp build_dict_task",
    "clean": "gulp clean_task",
    "coverage": "nyc --reporter=lcov mocha",
    "deploy": "npm run build-demo && npm run jsdoc && gulp deploy_task",
    "jsdoc": "gulp clean_jsdoc_task && jsdoc -c jsdoc.json -r src",
    "lint": "eslint src/**",
    "release": "gulp release_task",
    "test": "gulp test_task",
    "version": "gulp version_task",
    "watch": "gulp watch_task",
    "webserver": "npm run build-demo && npm run jsdoc && gulp webserver_task",
    "update-libraries": "npx npm-check-updates -u"
  }
}
